{
    "family": "financial-helper-programmer-agent",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::641870364246:role/financial-helper-task-exec",
    "taskRoleArn": "arn:aws:iam::641870364246:role/financial-helper-task",
    "containerDefinitions": [
        {
            "name": "programmer-agent",
                    "image": "641870364246.dkr.ecr.eu-west-3.amazonaws.com/financial-helper-agent:fixed",
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/financial-helper-agent",
                    "awslogs-region": "eu-west-3",
                    "awslogs-stream-prefix": "programmer-agent"
                }
            },
            "environment": [
                {
                    "name": "NODE_ENV",
                    "value": "production"
                },
                {
                    "name": "GITHUB_TOKEN",
                    "value": "PLACEHOLDER_GITHUB_TOKEN"
                },
                {
                    "name": "GH_OWNER",
                    "value": "PLACEHOLDER_GH_OWNER"
                },
                {
                    "name": "GH_REPO",
                    "value": "PLACEHOLDER_GH_REPO"
                },
                {
                    "name": "ISSUE_NUMBER",
                    "value": "PLACEHOLDER_ISSUE_NUMBER"
                },
                {
                    "name": "ASSIGNEE",
                    "value": "ai-bot"
                },
                {
                    "name": "DEFAULT_BRANCH",
                    "value": "main"
                },
                {
                    "name": "POLICY_PATH",
                    "value": "/workspace/policy.yaml"
                }
            ]
        }
    ]
}