{
  "name": "crypto-trade-dashboard",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev:api": "pnpm --filter @apps/api dev",
    "dev:web": "pnpm --filter @apps/web dev",
    "dev:bot": "pnpm --filter @apps/bot dev",
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "turbo": "2.1.3",
    "typescript": "^5.6.2",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "vitest": "^2.1.1"
  },
  "pnpm": {
    "overrides": {}
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}