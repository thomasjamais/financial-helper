{
  "name": "crypto-trade-dashboard",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev:api": "pnpm --filter @apps/api dev",
    "dev:web": "pnpm --filter @apps/web dev",
    "dev:bot": "pnpm --filter @apps/bot dev",
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "agent:build": "tsc -p agent/tsconfig.json",
    "agent:dev:programmer": "tsx agent/src/index.ts",
    "agent:dev:product": "tsx agent/src/product/index.ts",
    "db:up": "docker compose -f infra/compose.yaml up -d postgres redis",
    "db:down": "docker compose -f infra/compose.yaml down",
    "db:logs": "docker compose -f infra/compose.yaml logs -f postgres",
    "db:reset": "docker compose -f infra/compose.yaml down -v && docker compose -f infra/compose.yaml up -d postgres redis"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "turbo": "2.1.3",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "pnpm": {
    "overrides": {}
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "bcrypt": "^6.0.0",
    "jsonwebtoken": "^9.0.2"
  }
}